/* pages/admin/orders/orderList.wxss */
.container {
  display:flex;
  flex-direction:column;
  font-size: 12px;
  margin:20rpx 40rpx;
}

.list {
  width: 100%;
}

.order-item-wrap {
  border-bottom:1px solid #d8d8d8;
  display:flex;
  padding: 15rpx 0;
}

.content {
  flex: 1;
}

.order-wrap {
  display: flex;
  justify-content: space-between;
  font-size:14px;
  color:#dc143c;
}

.status {
  display: flex;
  justify-content: space-between;
}